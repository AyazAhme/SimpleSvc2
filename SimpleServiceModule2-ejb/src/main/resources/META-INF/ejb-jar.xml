<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC
      "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN"
      "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
  <display-name>SimpleServiceEJB</display-name>
  <enterprise-beans>
    <session>
      <ejb-name>SimpleService</ejb-name>
      <home>com.orgn.teg.SimpleServiceHome</home>
      <remote>com.orgn.teg.SimpleServiceRemote</remote>
      <ejb-class>com.orgn.teg.SimpleServiceBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
  </enterprise-beans>

  <assembly-descriptor>
  </assembly-descriptor>

</ejb-jar>
